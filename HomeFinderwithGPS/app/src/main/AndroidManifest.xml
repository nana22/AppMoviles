        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyAacE5tmo3TJ62eor5Sze3EmTVNHoT3XG0  "/>

        <activity
            android:name="com.example.contactlist.homefinder.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.contactlist.homefinder.app.Ver"
            android:label="@string/title_activity_ver" >
        </activity>
        <activity
            android:name="com.example.contactlist.homefinder.app.Mapa"
            android:label="Mapa" >
        </activity>
        <activity
            android:name="com.example.contactlist.homefinder.app.Registrar"
            android:label="@string/title_activity_registrar" >
        </activity>
        <activity
            android:name="com.example.contactlist.homefinder.app.ACDetalles"
            android:label="@string/title_activity_acdetalles" >
        </activity>
    </application>

</manifest>
